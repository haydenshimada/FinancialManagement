<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="70.0" prefWidth="350.0" style="-fx-background-color: #2A3139;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.financial.HistoryTypeController">
   <children>
      <Button fx:id="button" layoutX="14.0" layoutY="28.0" mnemonicParsing="false" style="-fx-background-color: orange;" stylesheets="@../Style/Button.css">
