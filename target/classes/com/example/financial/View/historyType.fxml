            <ImageView fx:id="image" fitHeight="21.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/newType/shopping.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="typeLabel" layoutX="54.0" layoutY="32.0" text="Shopping" textFill="WHITE">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Label>
      <Label fx:id="date" contentDisplay="RIGHT" layoutX="137.0" layoutY="5.0" text="19/12/2021" textAlignment="RIGHT" textFill="WHITE">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Label>
      <Label fx:id="money" alignment="CENTER_RIGHT" layoutX="147.0" layoutY="32.0" prefHeight="22.0" prefWidth="189.0" text="1.000.000" textAlignment="RIGHT" textFill="WHITE">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Label>
