<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="46.0" prefWidth="350.0" style="-fx-background-color: #2A3139;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.financial.EditTypeController">
   <children>
      <Button fx:id="button" layoutX="14.0" layoutY="8.0" mnemonicParsing="false" style="-fx-background-color: orange;" stylesheets="@../Style/Button.css">
         <graphic>
            <ImageView fx:id="image" fitHeight="21.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Icon/newType/shopping.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fitHeight="30.0" fitWidth="24.0" layoutX="312.0" layoutY="11.0" onMouseClicked="#deleteClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Icon/bin.png" />
         </image>
      </ImageView>
      <Label fx:id="label" layoutX="54.0" layoutY="12.0" text="Shopping" textFill="WHITE">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
